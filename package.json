{
  "name": "@yc-w-cn/douyin-mcp-server",
  "version": "1.0.1",
  "description": "抖音视频解析与下载工具，支持 MCP 协议",
  "main": "dist/index.js",
  "bin": {
    "douyin-mcp-server": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "keywords": [
    "mcp",
    "douyin",
    "tiktok",
    "video",
    "download",
    "ai",
    "automation"
  ],
  "author": {
    "name": "Yuchen Wang",
    "email": "contact@wangyuchen.cn",
    "url": "https://github.com/yc-w-cn"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yc-w-cn/douyin-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/yc-w-cn/douyin-mcp-server/issues"
  },
  "homepage": "https://github.com/yc-w-cn/douyin-mcp-server#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "axios": "^1.13.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "packageManager": "pnpm@10.26.2"
}
